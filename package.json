{
  "name": "hangmanify",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "start:storybook": "cross-env STORYBOOK_ENABLED=true expo start --reset-cache",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "format:lint": "eslint .",
    "format:prettier": "prettier . --write",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "docz:serve": "docz serve",
    "check:ts": "tsc",
    "check:deps": "expo install --check",
    "check:expo": "npx expo-doctor@latest .",
    "check:pkg": "sh scripts/yarn/analyze-deps.sh",
    "check:vscode-extensions": "code --list-extensions | jq -R . | jq -s '{\"recommendations\": .}' > .vscode/extensions.json",
    "prepare": "husky",
    "storybook-generate": "sb-rn-get-stories",
    "install-vscode-extensions": "cd .vscode && chmod +x install && ./install",
    "test": "jest -u"
  },
  "engines": {
    "node": ">=22",
    "npm": ">=10",
    "yarn": ">=1.2"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^5.1.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.2.0",
    "@react-native-community/slider": "4.5.5",
    "@react-navigation/native": "^7.0.19",
    "@react-navigation/native-stack": "^7.3.3",
    "expo": "~52.0.46",
    "expo-asset": "~11.0.5",
    "expo-constants": "^17.0.8",
    "expo-dev-client": "~5.0.20",
    "expo-font": "^13.0.4",
    "expo-localization": "^16.0.1",
    "expo-splash-screen": "~0.29.24",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.9",
    "expo-video": "~2.0.6",
    "expo-video-thumbnails": "~9.0.3",
    "i18n-js": "^4.5.1",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-native": "0.76.9",
    "react-native-gesture-handler": "2.20.2",
    "react-native-reanimated": "3.16.7",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@emotion/core": "10.1.1",
    "@eslint/js": "^9.23.0",
    "@google/generative-ai": "^0.24.0",
    "@storybook/addon-ondevice-actions": "^8.6.2",
    "@storybook/addon-ondevice-controls": "^8.6.2",
    "@storybook/react-native": "^8.6.2",
    "@tailwindcss/postcss": "^4.1.2",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/react": "~18.3.12",
    "babel-loader": "^10.0.0",
    "cross-env": "^7.0.3",
    "docz": "^2.4.0",
    "eslint": "^9.23.0",
    "eslint-plugin-perfectionist": "^4.13.0",
    "eslint-plugin-react": "^7.37.4",
    "fast-levenshtein": "^3.0.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.5.0",
    "memfs": "^4.17.2",
    "postcss": "^8.5.3",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react-art": "18.3.1",
